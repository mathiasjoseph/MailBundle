<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
<services>
    <service class="Miky\Bundle\MailBundle\Manager\PredefinedMailManager" id="miky_predefined_mail_manager">
        <argument id="doctrine.orm.entity_manager" type="service"/>
        <argument>%miky_mail.model.predefined_mail.class%</argument>
    </service>
    <service class="Miky\Bundle\MailBundle\Model\PredefinedSchemaManager" id="miky_predefined_schema_manager">
        <argument id="miky.registry.mail_group_predefined_schema" type="service"/>
    </service>
</services>
</container>
